mixin component-cards-list-carousel(content)

	each item,i in content

		- var classes = item.classes || ''

		+fragment("fragment-card",{"src":item.image.src,"alt":item.image.alt,"header":item.header,"description":item.content,"button":{"href":item.button.href,"title":item.button.title,"text":item.button.text},"classes":classes})