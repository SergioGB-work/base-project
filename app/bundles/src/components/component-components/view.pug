							+component({"name":"component-navigation","content":{"pages":[{"text":"Pagina 1","href":"/pagina1.html","title":"Ir a pagina 1","childs":[{"text":"Pagina 1-1","href":"/pagina1-1.html","title":"Ir a pagina 1-1"}]},{"text":"Pagina 2","href":"/pagina2.html","title":"Ir a pagina 2","childs":[]}],"view":"pills"},"showTitle":"false","full":"true"})
					.row
						.col-12.col-md-6
							h4 Pills Justified
							+component({"name":"component-navigation","content":{"pages":[{"text":"Pagina 1","href":"/pagina1.html","title":"Ir a pagina 1","childs":[],"active":"true"},{"text":"Pagina 2","href":"/pagina2.html","title":"Ir a pagina 2","childs":[]}],"view":"pills","justified":"true"},"showTitle":"false","full":"true"})

						.col-12.col-md-6
