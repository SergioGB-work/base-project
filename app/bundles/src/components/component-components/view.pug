					p ${{ components.paragraph2 }}$

					p ${{ components.paragraph3 }}$

					p ${{ components.paragraph4 }}$

					p ${{ components.paragraph5 }}$

					P ${{ components.paragraph6 }}$


			table(class="table table-responsive")
				thead
					tr
						th ${{ components.param }}$
						th ${{ components.description }}$
						th ${{ components.default }}$
				tbody
					tr
						td <strong>name</strong>
						td ${{ components.componentTypeDescription }}$
