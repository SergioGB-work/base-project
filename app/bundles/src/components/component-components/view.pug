		.col-12
			.card
				.card-body
					h3(id="componentHighlightedContent").m-0 Highlighted Content
			.row
				.col-md-12
					+component({"name":"component-highlighted-content","content":{"content":"<h2>${{ default.homeOwnGeneratorsTitle }}$</h2><h3>${{ default.homeOwnGeneratorsSubTitle }}$</h3><p>${{ default.homeOwnGeneratorsContent }}$</p>","button":{"text":"Click!","href":"http://www.google.es","title":"Ir a Google"}},"showTitle":"false","full":"true"})

		.col-12
			.card
				.card-body
					h3(id="componentHighlightedFlips").m-0 Highlighted Flips
			.row
				.col-md-12
					+component({"name":"component-highlighted-flips","content":[{"header":"Header1","content":"Lorem ipsum dolor sit", "iconType":"fontawesome", "iconCode":"fa-ellipsis-v", "iconSize":"icon-6x"},{"header":"Header2","content":"Lorem ipsum dolor sit", "iconType":"fontawesome", "iconCode":"fa-align-center", "iconSize":"icon-6x"},{"header":"Header3","content":"Lorem ipsum dolor sit", "iconType":"material", "iconCode":"&#xE84F;", "iconSize":"icon-6x"}],"showTitle":"false","full":"true"})
		.col-12
			.card
				.card-body
					h3(id="componentHighlightedSideImage").m-0 Highlighted Side Image
