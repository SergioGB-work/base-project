							+component({"name":"component-navigation","content":{"pages":[{"text":"Pagina 1","href":"/pagina1.html","title":"Ir a pagina 1","childs":[],"active":"true"},{"text":"Pagina 2","href":"/pagina2.html","title":"Ir a pagina 2","childs":[]}],"view":"tabs"},"showTitle":"false","full":"true"})
