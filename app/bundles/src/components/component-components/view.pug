								li <strong>description:</strong> ${{ components.descriptionText }}$
								li <strong>src:</strong> ${{ components.imageURL }}$
								li <strong>alt:</strong> ${{ components.altText }}$
								li <strong>href:</strong> ${{ components.cardLink }}$
								li <strong>title:</strong> ${{ components.buttonTitle }}$
								li <strong>classes:</strong> ${{ components.cardClasses }}$
								li <strong>price:</strong> ${{ components.cardPrice }}$

					h4.icon-2x ${{ components.example }}$
					pre
						| +component({"name":"component-cards-list-carousel","content":{"items":[{"header":"Header 1","description":"Lorem ipsum","src":"./images/monument.jpg","alt":"Image 1","href":"http://google.com","title":"Ir a SunnieJS","classes":"full m-0"},{"header":"Header 2","description":"Lorem ipsum","src":"./images/monument.jpg","alt":"Image 1","href":"http://google.com","title":"Ir a SunnieJS","classes":"full m-0"},{"header":"Header 3","description":"Lorem ipsum","src":"./images/monument.jpg","alt":"Image 1","href":"http://google.com","title":"Ir a SunnieJS","classes":"full m-0"},{"header":"Header 4","description":"Lorem ipsum","src":"./images/monument.jpg","alt":"Image 1","href":"http://google.com","title":"Ir a SunnieJS","classes":"full m-0"},{"header":"Header 5","description":"Lorem ipsum","src":"./images/monument.jpg","alt":"Image 1","href":"http://google.com","title":"Ir a SunnieJS","classes":"full m-0"}]},"showTitle":"false","full":"true"})
			.card
				.card-body
					h3.icon-4x
